<application xmlns="http://ns.adobe.com/air/application/17.0">
  <id>MultiMTG</id>
  <versionNumber>1.1.1</versionNumber>
  <versionLabel/>
  <filename>Multiplayer MTG</filename>
  <description/>
  <name>Multiplayer MTG</name>
  <copyright/>
  <initialWindow>
    <content>MTG-App.swf</content>
    <systemChrome>standard</systemChrome>
    <transparent>false</transparent>
    <visible>true</visible>
    <fullScreen>true</fullScreen>
    <aspectRatio>portrait</aspectRatio>
    <renderMode>auto</renderMode>
    <autoOrients>false</autoOrients></initialWindow>
  <icon>
    <image36x36>Images/icon36.png</image36x36>
    <image48x48>Images/icon48.png</image48x48>
    <image72x72>Images/icon72.png</image72x72>
    <image96x96>Images/icon96.png</image96x96>
  </icon>
  <customUpdateUI>false</customUpdateUI>
  <allowBrowserInvocation>false</allowBrowserInvocation>
  <supportedLanguages>en</supportedLanguages>
  <android>
    <manifestAdditions><![CDATA[<manifest>
<uses-permission android:name="android.permission.INTERNET"/>
<uses-permission android:name="android.permission.READ_PHONE_STATE"/>
<uses-permission android:name="android.permission.DISABLE_KEYGUARD"/>
<uses-permission android:name="android.permission.WAKE_LOCK"/>
<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
<uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
</manifest>]]></manifestAdditions>
  </android>
</application>
