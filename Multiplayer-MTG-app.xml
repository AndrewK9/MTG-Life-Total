<application xmlns="http://ns.adobe.com/air/application/23.0">
  <id>MultiplayerMagic</id>
  <versionNumber>1.2.4</versionNumber>
  <versionLabel/>
  <filename>Multiplayer-Magic</filename>
  <description/>
  <name>Multiplayer-Magic</name>
